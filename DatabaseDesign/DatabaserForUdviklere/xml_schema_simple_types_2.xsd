<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="cprs">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="cpr">
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:pattern value="[0-9]{2}(0[1-9]|1[012])(0[1-9]|[12][0-9]|3[01])\-[0-9]{4}"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>