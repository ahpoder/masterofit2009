<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="grades">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="grade">
					<xs:simpleType>
						<xs:restriction base="xs:token">
							<xs:pattern value="(\+?(A|B|C|D)\+?)|E|F|I|N"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>